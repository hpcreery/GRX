{
  "name": "grx",
  "version": "2.0.2",
  "description": "Fastest Web Geber Viewer",
  "main": "./out/main/index.js",
  "author": "Hunter Creery",
  "homepage": "https://grx.creery.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:web": "vite",
    "build": "npm run typecheck && electron-vite build",
    "build:web": "vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "build:all": "npm run build && electron-builder --win --mac --linux --config",
    "prepare": "electron-builder install-app-deps && husky install",
    "create:icons": "electron-icon-builder --input=build/icon.png --output=build --flatten && mv build/icons/* build && rm -rf build/icons"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@emotion/react": "^11.10.6",
    "@hpcreery/pixi-viewport": "^5.0.1",
    "@hpcreery/tracespace-parser": "^5.0.0-alpha.11",
    "@hpcreery/tracespace-plotter": "^5.0.0-alpha.11",
    "@hpcreery/tracespace-renderer": "^5.0.0-alpha.11",
    "@pixi-essentials/cull": "^2.0.0",
    "@pixi/display": "^7.2.3",
    "@pixi/unsafe-eval": "^7.2.4",
    "@pixi/webworker": "^7.2.4",
    "@react-hook/resize-observer": "^1.2.6",
    "@tracespace/parser": "^5.0.0-alpha.0",
    "@tracespace/plotter": "^5.0.0-alpha.0",
    "@use-gesture/react": "^10.2.26",
    "antd": "^5.4.4",
    "chroma-js": "^2.4.2",
    "comlink": "^4.4.1",
    "electron-updater": "^5.3.0",
    "pixi-graphpaper": "^2.0.1",
    "pixi-viewport": "^5.0.1",
    "pixi.js": "^7.2.4",
    "react-color": "^2.19.3",
    "tess2-ts": "^1.0.8",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@react-spring/web": "^9.7.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tracespace/fixtures": "5.0.0-alpha.0",
    "@types/chroma-js": "^2.4.0",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.38",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-comlink": "^3.0.5"
  }
}
