{
  "name": "grx",
  "version": "4.1.0",
  "description": "GPU-Accelerated Web Based EDA Manufacturing Artwork Viewer",
  "main": "./out/main/index.js",
  "author": "Hunter Creery",
  "homepage": "https://grx.creery.org",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:web": "vite",
    "build": "npm run typecheck && electron-vite build",
    "build:web": "vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "build:all": "npm run build && electron-builder --win --mac --linux --config",
    "build:prepare": "electron-builder install-app-deps && husky install",
    "create:icons": "electron-icon-builder --input=build/icon.png --output=build --flatten && mv build/icons/* build && rm -rf build/icons",
    "build:shaders": "node src/renderer/src/renderer/shaders/build.js",
    "build:shaders:watch": "node src/renderer/src/renderer/shaders/watch.js",
    "build:chevrotain:types": "tsx src/renderer/lib/nc/chevrotain-parser/gen_dts_signature.js",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hpcreery/tracespace-parser": "^5.0.4",
    "@mantine/carousel": "^8.3.5",
    "@mantine/code-highlight": "^8.3.5",
    "@mantine/core": "^8.3.5",
    "@mantine/dropzone": "8.3.5",
    "@mantine/form": "^8.3.5",
    "@mantine/hooks": "^8.3.5",
    "@mantine/modals": "^8.3.5",
    "@mantine/notifications": "^8.3.5",
    "@mantine/nprogress": "^8.3.5",
    "@mantine/spotlight": "^8.3.5",
    "mantine-contextmenu": "^8.2.0",
    "@react-hook/resize-observer": "^2.0.2",
    "@react-spring/web": "^10.0.3",
    "@tabler/icons-react": "^3.35.0",
    "@use-gesture/react": "^10.3.1",
    "chevrotain": "^11.0.3",
    "chroma-js": "^3.1.2",
    "comlink": "^4.4.2",
    "dxf-parser": "^1.1.2",
    "earcut": "^3.0.2",
    "gl-matrix": "^3.4.4",
    "glslify": "^7.1.1",
    "ndarray": "^1.0.19",
    "re-resizable": "^6.11.2",
    "react-color": "^2.19.3",
    "regl": "^2.1.1",
    "regl-stats-widget": "^0.0.1",
    "typescript-event-target": "^1.1.1",
    "web-vitals": "^5.1.0",
    "webgl-utils": "^1.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/notarize": "^3.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chroma-js": "^3.1.1",
    "@types/earcut": "^3.0.0",
    "@types/ndarray": "^1.0.14",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "electron-updater": "^6.6.2",
    "electron-vite": "^4.0.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "generate-changelog": "^1.8.0",
    "glslify-import": "^3.1.0",
    "husky": "^9.1.7",
    "npm-check-updates": "^19.1.1",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup-plugin-glslify": "^1.3.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-arraybuffer": "^0.1.1",
    "vite-plugin-comlink": "^5.3.0",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-glsl": "^1.5.4",
    "vitest": "^3.2.4"
  }
}
