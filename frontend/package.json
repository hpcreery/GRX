  "homepage": "./",
  "main": "src/electron.bundle.js",
  "scripts": {
    "start:server": "npx serve -s build -l 8080",
    "start:service": "pm2 --name ToolkitFrontend --watch --no-automation serve -s build -l 8080 --spa",
    "start:electron": "electron .",
    "dev": "nf start",
    "dev:electronwaitreact": "node src/electron-wait-react",
    "dev:electron": "electron ./src/electron.js",
    "dev:react": "node scripts/start.js",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "node scripts/build.js",
    "build:electron": "webpack --config config/webpackElectron.config.js",
    "pack": "electron-packager . electron --platform=win32  --arch=x64 --ignore=/node_modules --ignore=/dist --overwrite --icon=public/NVG.ico && xcopy /s /q /i public electron-win32-x64\\public && echo 'Built for WIN'",
    "release": "electron-builder -p always",
    "dist": "electron-builder --publish always",
    "start-update-server": "http-server dist/ -p 8083",
    "reinvent": "npm install && npm run build && npm run pack && npm run dist && npm run start-update-server"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.netvia.toolkit",
    "productName": "EngineerTookit",
    "extends": null,
    "directories": {
      "buildResources": "build"
    },
    "electronDist": "electron-win32-x64",
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "installerIcon": "public/icon.ico",
      "installerHeaderIcon": "public/icon.ico",
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://hunter-machine:8083",
        "channel": "latest",
        "publishAutoUpdate": true
      }
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "C:\\Users\\huntercreery\\AppData\\Roaming\\npm-cache\\_npx\\51116\\node_modules\\react-scripts\\node_modules\\jest-circus\\runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
